    <property name="mimetypes">text/plain</property>
    <property name="globs">*.pcm</property>
  </metadata>

  <styles>
    <style id="execute"  _name="Execute"   map-to="def:function" />
    <style id="comment"  _name="Comment"   map-to="def:comment" />
    <style id="string"   _name="String"    map-to="def:string" />
    <style id="command"  _name="Command"   map-to="def:keyword" />
    <style id="function" _name="Function"  map-to="def:builtin" />
    <style id="option"   _name="Option"    map-to="def:type" />
    <style id="label"    _name="Label"     map-to="def:preprocessor" />
  </styles>

  <default-regex-options case-sensitive="false" />

  <definitions>
