      <include>
        <context id="execute" style-ref="execute">
          <start>@[^,\s]*</start>
          <end>\s*</end>
        </context>
        <context id="comment" style-ref="comment">
          <start>!</start>
          <end>$</end>
        </context>
        <context id="string" end-at-line-end="true" style-ref="string">
          <start>`</start>
          <end>'</end>
        </context>
        <context id="label" style-ref="label">
          <match>\s*[^,:]*:$</match>
        </context>
        <context id="option" style-ref="option">
          <match>\\[^,\s]*\s</match>
        </context>

